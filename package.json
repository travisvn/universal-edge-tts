{
  "name": "universal-edge-tts",
  "version": "1.3.2",
  "description": "Alias package for edge-tts-universal - Universal text-to-speech library",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "browser": {
    "./index.js": "./browser.js",
    "./index.d.ts": "./browser.d.ts"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js",
      "types": "./index.d.ts"
    },
    "./browser": {
      "import": "./browser.js",
      "require": "./browser.js",
      "types": "./browser.d.ts"
    },
    "./isomorphic": {
      "import": "./isomorphic.js",
      "require": "./isomorphic.js",
      "types": "./isomorphic.d.ts"
    },
    "./webworker": {
      "import": "./webworker.js",
      "require": "./webworker.js",
      "types": "./webworker.d.ts"
    }
  },
  "scripts": {
    "test": "node test.js",
    "prepublishOnly": "npm test"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "browser.js",
    "browser.d.ts",
    "isomorphic.js",
    "isomorphic.d.ts",
    "webworker.js",
    "webworker.d.ts",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "edge-tts-universal": "^1.3.2"
  },
  "keywords": [
    "text-to-speech",
    "tts",
    "edge-tts",
    "universal",
    "alias",
    "microsoft",
    "speech-synthesis"
  ],
  "author": "Travis <contact@travis.engineer>",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travisvn/universal-edge-tts.git"
  },
  "homepage": "https://github.com/travisvn/universal-edge-tts#readme",
  "bugs": {
    "url": "https://github.com/travisvn/universal-edge-tts/issues"
  }
}
